<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Add_Main_Activity"
    tools:layout_editor_absoluteY="81dp">


    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="@color/white"
        android:layout_marginBottom="1dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Space
            android:layout_width="16dp"
            android:layout_height="10dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="470dp" />

        <LinearLayout
            android:id="@+id/Main_Add_Home"
            android:layout_width="71dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:tint="@color/gray"
                app:srcCompat="@drawable/ic_home_black_24dp" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Home"
                android:textColor="@color/gray"
                />
        </LinearLayout>

        <Space
            android:layout_width="17dp"
            android:layout_height="8dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="470dp" />

        <LinearLayout
            android:id="@+id/Main_Add_Add"
            android:layout_width="71dp"
            android:layout_height="match_parent"

            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView8437"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:tint="@color/orange"
                app:srcCompat="@drawable/write" />

            <TextView
                android:id="@+id/textView2443"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Add"
                android:textColor="@color/orange" />
        </LinearLayout>


        <Space
            android:layout_width="17dp"
            android:layout_height="13dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="470dp" />

        <LinearLayout
            android:id="@+id/Main_Add_chat"
            android:layout_width="71dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView87"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:tint="@color/gray"
                app:srcCompat="@drawable/ic_notifications_black_24dp" />

            <TextView
                android:id="@+id/textView244234"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Chat"
                android:textColor="@color/gray" />
        </LinearLayout>

        <Space
            android:layout_width="17dp"
            android:layout_height="13dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="470dp" />

        <LinearLayout
            android:id="@+id/Main_Add_Mypage"
            android:layout_width="71dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:tint="@color/gray"
                app:srcCompat="@drawable/person" />

            <TextView
                android:id="@+id/textView247567"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Mypage"
                android:textColor="@color/gray" />
        </LinearLayout>

    </LinearLayout>
    <View
        android:id="@+id/vivfew2"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_centerInParent="true"
        android:layout_gravity="center"
        android:layout_marginBottom="8dp"
        android:background="#2a2a2a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
    <LinearLayout
        android:layout_width="370dp"
        android:layout_height="493dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="164dp"
            android:gravity="center"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/imageView"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:src="@drawable/ic_menu_camera" />
            <Space
                android:layout_width="15dp"
                android:layout_height="6dp" />
            <LinearLayout
                android:id="@+id/linearLayout13"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone"
                android:orientation="vertical"
              >

                <android.support.v4.view.ViewPager
                    android:id="@+id/photos_viewpager2"
                    android:layout_width="match_parent"
                    android:layout_height="109dp">

                </android.support.v4.view.ViewPager>

                <android.support.design.widget.TabLayout
                    android:id="@+id/tab_layout3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:tabBackground="@drawable/tab_selector"
                    app:tabGravity="center"
                    app:tabIndicatorHeight="0dp" />


            </LinearLayout>
        </LinearLayout>

        <Space
            android:layout_width="18dp"
            android:layout_height="8dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">


            <EditText
                android:id="@+id/editText5"
                android:layout_width="319dp"
                android:layout_height="40dp"
                android:background="@drawable/line_btn"
                android:ems="10"
                android:fontFamily="@font/ko2"
                android:textColor="#2a2a2a"
                android:hint="글제목을 입력해주세요"
                android:inputType="textPersonName"
                android:textSize="15dp" />

        </LinearLayout>

        <Space
            android:layout_width="8dp"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Space
                android:layout_width="27dp"
                android:layout_height="8dp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="나의위치:"
                android:fontFamily="@font/ko2"
                android:textColor="#3b3b3b"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/location_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:fontFamily="@font/ko2"
                android:textColor="#3b3b3b"
                android:textSize="12dp"
                android:textStyle="bold" />


        </LinearLayout>
        <Space
            android:layout_width="18dp"
            android:layout_height="10dp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Space
                android:layout_width="27dp"
                android:layout_height="8dp" />

            <EditText
                android:id="@+id/editText7"
                android:layout_width="220dp"
                android:background="@drawable/line_btn"
                android:layout_height="40dp"
                android:ems="10"
                android:textSize="15dp"
                android:fontFamily="@font/ko2"
                android:textColor="#2a2a2a"
                android:hint="가격을 입력해주세요"
                android:inputType="textPersonName" />

            <Space
                android:layout_width="18dp"
                android:layout_height="8dp" />

            <CheckBox
                android:id="@+id/checkBox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/ko2"
                android:textColor="#2a2a2a"
                android:text="흥정여부" />
        </LinearLayout>

        <Space
            android:layout_width="8dp"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="116dp"
            android:orientation="horizontal">

            <Space
                android:layout_width="27dp"
                android:layout_height="8dp" />

            <EditText
                android:id="@+id/editText9"
                android:layout_width="319dp"
                android:fontFamily="@font/ko2"
                android:layout_height="match_parent"
                android:background="@drawable/line_btn"
                android:ems="4"
                android:gravity="top"
                android:textColor="#2a2a2a"
                android:hint="게시글 내용을 작성해주세요"
                android:inputType="textMultiLine"
                android:textSize="15dp" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="88dp"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/fragment_two_btn"
                android:layout_width="318dp"
                android:layout_height="46dp"
                android:background="@drawable/button"
                android:text="물품등록"
                android:textSize="16dp"
                android:fontFamily="@font/ko3"
                android:textColor="@color/white"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>